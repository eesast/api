{
  "name": "eesast-api",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "debug": "nodemon --debug",
    "build": "babel src -d build -x \".ts\" --ignore \"src/@types/**/*\" --source-maps",
    "serve": "cross-env NODE_ENV=production node build/index.js",
    "lint": "eslint --ext .js,.ts src/",
    "typecheck": "tsc --noUnusedLocals"
  },
  "dependencies": {
    "@babel/runtime": "7.21.0",
    "@types/node-cron": "3.0.7",
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "dockerode": "3.3.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "fs": "^0.0.1-security",
    "graphql": "16.6.0",
    "graphql-request": "4.3.0",
    "graphql-tag": "2.12.6",
    "html-to-text": "8.2.1",
    "isemail": "3.2.0",
    "jsonwebtoken": "9.0.0",
    "mathjs": "10.6.4",
    "mongoose": "6.8.4",
    "morgan": "1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "3.0.2",
    "node-fetch": "2.6.9",
    "nodemailer": "6.9.1",
    "qcloud-cos-sts": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.4",
    "@babel/node": "7.20.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-typescript": "7.21.4",
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.13",
    "@types/debug": "4.1.7",
    "@types/dockerode": "3.3.16",
    "@types/express": "4.17.17",
    "@types/html-to-text": "8.1.1",
    "@types/jsonwebtoken": "8.5.9",
    "@types/mongodb": "4.0.6",
    "@types/mongoose": "5.10.3",
    "@types/morgan": "1.9.4",
    "@types/multer": "1.4.7",
    "@types/node-fetch": "2.6.2",
    "@types/nodemailer": "6.4.7",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.6.0",
    "husky": "7.0.4",
    "lint-staged": "12.5.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.7",
    "typescript": "4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint && yarn typecheck"
    }
  },
  "lint-staged": {
    "{src/**/*,test/**/*}.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "nodemonConfig": {
    "env": {
      "DEBUG": "eesast-api"
    },
    "watch": [
      "src"
    ],
    "ext": "ts,json",
    "ignore": [
      "src/**/*.test.ts"
    ],
    "exec": "babel-node --extensions \".ts\" --config-file \"./.babelrc\" src/index.ts"
  },
  "version": "0.0.0"
}
